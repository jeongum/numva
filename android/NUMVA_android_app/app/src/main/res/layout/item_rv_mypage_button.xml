<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="53dp"
    android:background="@drawable/roundrect_white_background"
    android:elevation="10dp"
    android:gravity="center"
    android:layout_marginHorizontal="36dp"
    android:layout_marginBottom="14dp"

    xmlns:app="http://schemas.android.com/apk/res-auto">
    <TextView
        android:id="@+id/mypage_btn_tv_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="로그아웃"
        android:textSize="17dp"
        android:fontFamily="@font/notosanscjkk_regular"
        android:textColor="#3B476B"
        android:layout_marginStart="17dp"
        android:layout_marginTop="14dp"
        android:includeFontPadding="false"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <ImageView
        android:id="@+id/mypage_btn_iv_enter"
        android:layout_width="14dp"
        android:layout_height="12dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:src="@drawable/ic_right_arrow_primary"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>


</androidx.constraintlayout.widget.ConstraintLayout>